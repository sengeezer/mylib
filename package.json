{
  "name": "my-lib-a",
  "version": "0.0.0-development",
  "description": "My own javascript library",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sengeezer/mylib.git"
  },
  "keywords": [
    "own",
    "js",
    "javascript",
    "library"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Fabio M. Albertin <fabio@albertin.co.uk> (http://ifab.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sengeezer/mylib/issues"
  },
  "homepage": "https://github.com/sengeezer/mylib#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-latest": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "rimraf": "2.5.4",
    "semantic-release": "^6.3.2"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "latest",
      "stage-3"
    ]
  }
}
